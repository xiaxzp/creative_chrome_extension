    "webextension-polyfill": "^0.10.0",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-ext-reloader": "^1.1.12",
    "ws": "^8.5.0"
  },
  "standard-version": {
    "bumpFiles": [
      "package.json"
    ]
  }
}
