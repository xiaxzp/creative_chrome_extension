{
  "name": "creative-devtool-demo",
  "displayName": "Creative Devtool Demo",
  "version": "1.2.1",
  "private": true,
  "description": "Empower Creative FE Team",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "npm run clear && run-p dev:*",
    "dev:prepare": "tsx scripts/prepare.ts",
    "dev:webpack": "cross-env NODE_ENV=development tsx ./node_modules/webpack/bin/webpack.js --config webpack.config.ts --watch",
    "build": "cross-env NODE_ENV=production run-s clear build:*",
    "build-test": "cross-env NODE_ENV=development run-s clear build:prepare build:webpack",
    "build:prepare": "tsx scripts/prepare.ts",
    "build:webpack": "cross-env NODE_ENV=production tsx ./node_modules/webpack/bin/webpack.js --config webpack.config.ts",
    "release": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "pack": "cross-env NODE_ENV=production run-p pack:*",
    "pack:zip": "rimraf dist.zip && VER=$(node -pe \"require('./package.json')['version']\") && zip -vr Creative_FE_Devtool.v$VER.zip dist/ -x \"*.DS_Store\"",
    "pack:crx": "crx pack dist -o ./dist.crx",
    "bundle:dry": "pnpm build && pnpm pack:zip",
    "bundle:release-patch": "pnpm release && pnpm build && pnpm pack:zip",
    "start:chromium": "web-ext run --source-dir ./dist --target=chromium",
    "clear": "rimraf dist && mkdir dist",
    "lint": "eslint 'src/**/*.{json,ts,tsx,js,vue}'"
  },
  "dependencies": {
    "@types/webpack-env": "^1.18.4",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@vueuse/components": "^9.3.0",
    "@vueuse/motion": "2.0.0-beta.12",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.0",
    "element-plus": "2.1.9",
    "eslint": "^9.0.0",
    "eventemitter3": "^4.0.7",
    "lodash-es": "^4.17.21",
    "moment": "2.30.1",
    "query-string": "^7.0.1",
    "rxjs": "^7.8.1",
    "sortablejs": "^1.14.0",
    "urlpattern-polyfill": "^4.0.3",
    "v-collapse-it": "^0.1.0",
    "vite": "^5.1.4",
    "vue-json-pretty": "^2.1.0",
    "vue-query": "^1.26.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.14.0",
    "@ffflorian/jszip-cli": "^3.1.5",
    "@iconify/json": "^1.1.398",
    "@types/fs-extra": "^9.0.12",
    "@types/node": "^16.7.13",
    "@types/sortablejs": "^1.10.7",
    "@types/webextension-polyfill": "^0.10.7",
    "@types/webpack": "^5.28.5",
    "@unocss/postcss": "^0.58.5",
    "@unocss/preset-wind": "^0.58.5",
    "@unocss/webpack": "^0.58.5",
    "@vue-macros/reactivity-transform": "^0.4.3",
    "@vue/compiler-sfc": "^3.4.20",
    "@vueuse/core": "^10.8.0",
    "autoprefixer": "^10.4.2",
    "chokidar": "^3.5.2",
    "chrome-types": "^0.1.68",
    "circular-dependency-plugin": "^5.2.2",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "crx": "^5.0.1",
    "css-loader": "^6.10.0",
    "esbuild-loader": "^4.0.3",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "fs-extra": "^10.0.0",
    "html-webpack-plugin": "^5.6.0",
    "kolorist": "^1.5.0",
    "mini-css-extract-plugin": "^2.8.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "postcss-loader": "^8.1.0",
    "postcss-nested": "^6.0.1",
    "resolve-url-loader": "^5.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^4.12.0",
    "rollup-plugin-copy": "^3.4.0",
    "sass": "^1.43.2",
    "sass-loader": "^14.1.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "standard-version": "^9.5.0",
